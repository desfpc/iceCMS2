{
  "name": "desfpc/icecms2",
  "description": "IceCMS2 - lite PHP 8.1 + MySQL 8 + Vue.js Framework and CMS",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Sergey Peshalov",
      "email": "desfpc@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.2",
    "ext-mysqli": "*",
    "ext-json": "*",
    "ext-gd": "*",
    "ext-redis": "*",
    "ext-zend-opcache": "*",
    "ext-curl": "*",
    "desfpc/visualijoper": "2.*",
    "desfpc/vuebootstrap": "1.05",
    "phpmailer/phpmailer": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.21",
    "phpmd/phpmd": "2.12.0"
  },
  "autoload": {
    "psr-4": {
      "iceCMS2\\": "vendor/desfpc/icecms2/",
      "app\\": "classes/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
